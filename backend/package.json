{
  "name": "vida-leve-api",
  "version": "1.0.0",
  "main": "dist/server.js",
  "license": "Attribution-NonCommercial-NoDerivatives 4.0 International",
  "scripts": {
    "build": "rimraf ./dist && tsc --project tsconfig.json",
    "start": "node dist/server.js",
    "dev": "ts-node-dev src/server.ts",
    "migration:run": "npx sequelize-cli db:migrate",
    "migration:revert": "npx sequelize-cli db:migrate:undo",
    "migration:seed": "npx sequelize-cli db:seed:all",
    "db:create": "npx sequelize-cli db:create",
    "db:setup": "npm run db:create && npm run migration:run && npm run migration:seed"
  },
  "keywords": [],
  "author": "",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.13.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/helmet": "^4.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.5.5",
    "rimraf": "^6.0.1",
    "sequelize-cli": "^6.6.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.2"
  }
}
